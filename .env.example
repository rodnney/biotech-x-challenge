# Configuração para Desenvolvimento Local
# Copie para .env e ajuste conforme necessário

# Database
DATABASE_URL=postgresql://admin_user:ChangeMeInProduction123!@localhost:5432/biotech_db

# AWS LocalStack
AWS_ENDPOINT_URL=http://localhost:4566
AWS_ACCESS_KEY_ID=demo-access-key
AWS_SECRET_ACCESS_KEY=demo-secret-key
AWS_DEFAULT_REGION=us-east-1

# S3 Buckets
S3_INPUT_BUCKET=biotech-input-local
S3_OUTPUT_BUCKET=biotech-output-local

# Redis
REDIS_URL=redis://localhost:6379

# MinIO (S3-compatible)
MINIO_ENDPOINT=http://localhost:9000
MINIO_ACCESS_KEY=minioadmin
MINIO_SECRET_KEY=minioadmin123

# Application
ENVIRONMENT=development
DEBUG=true
LOG_LEVEL=INFO

# Frontend
NEXT_PUBLIC_API_URL=http://localhost:8000
NEXT_PUBLIC_ENVIRONMENT=development
